.border_box {
  width: 100%;
  height: 98rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: fixed;
  left: 0px;
  bottom: calc(50rpx + env(safe-area-inset-bottom) / 2);
  z-index: 100;
  pointer-events: none;
}
.border_box .tabBar_miden_border {
  width: 100rpx;
